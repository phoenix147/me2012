<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="csv" nsURI="http://csvml_metamodel/1.0" nsPrefix="csvml_metamodel">
  <eClassifiers xsi:type="ecore:EClass" name="FileDefinitionSet">
    <eStructuralFeatures xsi:type="ecore:EReference" name="comprises" lowerBound="1"
        upperBound="-1" eType="#//FileDefinition" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="FileDefinition">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="contains" lowerBound="1"
        upperBound="-1" eType="#//FieldDefinition" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="FieldDefinition" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Index" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="FieldType" eType="#//FieldType"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="StaticField" eSuperTypes="#//FieldDefinition"/>
  <eClassifiers xsi:type="ecore:EClass" name="DerivedField" abstract="true" eSuperTypes="#//FieldDefinition"/>
  <eClassifiers xsi:type="ecore:EEnum" name="FieldType">
    <eLiterals name="String"/>
    <eLiterals name="Integer" value="1"/>
    <eLiterals name="Boolean" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AggregationField" eSuperTypes="#//DerivedField">
    <eStructuralFeatures xsi:type="ecore:EReference" name="operands" upperBound="-1"
        eType="#//FieldDefinition"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Operation" eType="#//AggregationOperation"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="RuleBasedFields" eSuperTypes="#//DerivedField">
    <eStructuralFeatures xsi:type="ecore:EReference" name="basedOn" upperBound="-1"
        eType="#//Rule"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="DefaultValue" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="AggregationOperation">
    <eLiterals name="sum"/>
    <eLiterals name="average" value="1"/>
    <eLiterals name="minimum" value="2"/>
    <eLiterals name="maximum" value="3"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Rule" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="ResultValue" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ComparisonRule" eSuperTypes="#//Rule">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="ComparisonValue" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Operator" eType="#//ComparisonOperator"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="EReference0" eType="#//FieldDefinition"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="compares" lowerBound="1"
        eType="#//FieldDefinition"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CompositeRule" eSuperTypes="#//Rule">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Operator" eType="#//CompositeOperator"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="compose" lowerBound="1"
        upperBound="-1" eType="#//Rule"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="ComparisonOperator">
    <eLiterals name="Greater"/>
    <eLiterals name="GreaterOrEquals" value="1"/>
    <eLiterals name="Lower" value="2"/>
    <eLiterals name="LowerOrEquals" value="3"/>
    <eLiterals name="Unequals" value="4"/>
    <eLiterals name="Equals" value="5"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="CompositeOperator">
    <eLiterals name="AND"/>
    <eLiterals name="OR" value="1"/>
    <eLiterals name="NOT" value="2"/>
  </eClassifiers>
</ecore:EPackage>
